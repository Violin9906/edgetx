{
    // "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.default.intelliSenseMode": "linux-gcc-arm",
    "C_Cpp.default.compileCommands": "${workspaceFolder}/build/arm-none-eabi/compile_commands.json",
    "cmake.generator": "Unix Makefiles",
    "cmake.configureSettings": {
        "CMAKE_C_COMPILER": "/opt/gcc-arm-none-eabi/bin/arm-none-eabi-gcc",
        "CMAKE_CXX_COMPILER": "/opt/gcc-arm-none-eabi/bin/arm-none-eabi-g++",
        "CMAKE_TRY_COMPILE_TARGET_TYPE": "STATIC_LIBRARY",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "PCB": "C7MINI",
        "PCBREV": "C7MINI",
        "DEFAULT_MODE": "2",
        "HELI": "OFF",
        "FLIGHT_MODES": "OFF",
        "CURVES": "OFF",
        "GVARS": "OFF",
        "WATCHDOG": "OFF",
        "LUA": "OFF",
        "LUA_MIXER": "OFF",
        "BOOTLOADER": "OFF",
    },
    "cmake.sourceDirectory": "${workspaceFolder}",
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "C_Cpp.intelliSenseEngine": "default",
    "cmake.deleteBuildDirOnCleanConfigure": true,
}